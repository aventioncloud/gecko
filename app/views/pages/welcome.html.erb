 <body ng-cloak="" data-url="<%=request.host%>" ng-class="{ 'layout-fixed' : app.layout.isFixed, 'layout-boxed' : app.layout.isBoxed, 'layout-dock' : app.layout.isDocked, 'layout-material': app.layout.isMaterial, 'aside-offscreen' : app.sidebar.isOffscreen, 'aside-mini' : app.sidebar.isMini, 'aside-right' : app.sidebar.isRight, 'footer-hidden': app.footer.hidden, 'in-app': !$state.includes('page')}">

   <div data-ui-view="" data-autoscroll="false" ng-controller="CoreController" class="app-container"></div>
   <!-- Settings-->
   <div ng-include="'templates/settings.html'" ng-class="{'visible': showSettings}" ng-init="showSettings = false" class="settings-wrapper"></div>
   <!-- End Settings-->
   <!-- Layer Morph-->
   <div class="layer-morph-overlay">
      <div class="layer-morph-inner bg-info"></div>
   </div>
   <div layer-morph-close="" class="layer-morph-close">
      <em class="icon-cross"></em>
   </div>
   <div class="layer-morph-container bg-info">
      <div class="layer-morph-wrapper">
         <div id="layer-search" ng-include="'templates/layer-search.html'" class="layer-morph"></div>
         <div class="layer-morph-footer">Naut</div>
      </div>
   </div>
   <!-- End Layer Morph-->
   <!-- build:vendor app/js/vendor.js-->
   <script src="/assets/modernizr/modernizr.js"></script>
   <script src="/assets/jquery/dist/jquery.js"></script>
   <script src="/assets/angular/angular.js"></script>
   <script src="/assets/angular-route/angular-route.js"></script>
   <script src="/assets/angular-cookies/angular-cookies.js"></script>
   <script src="/assets/angular-animate/angular-animate.js"></script>
   <script src="/assets/angular-touch/angular-touch.js"></script>
   <script src="/assets/angular-sanitize/angular-sanitize.js"></script>
   <script src="/assets/angular-resource/angular-resource.js"></script>
   <script src="/assets/angular-translate/angular-translate.js"></script>
   <script src="/assets/ngstorage/ngStorage.js"></script>
   <script src="/assets/angular-ui-router/release/angular-ui-router.js"></script>
   <script src="/assets/angular-ui-utils/ui-utils.js"></script>
   <script src="/assets/angular-translate-loader-url/angular-translate-loader-url.js"></script>
   <script src="/assets/angular-translate-loader-static-files/angular-translate-loader-static-files.js"></script>
   <script src="/assets/angular-translate-storage-local/angular-translate-storage-local.js"></script>
   <script src="/assets/angular-translate-storage-cookie/angular-translate-storage-cookie.js"></script>
   <script src="/assets/angular-bootstrap/ui-bootstrap-tpls.js"></script>
   <script src="/assets/angular-dynamic-locale/dist/tmhDynamicLocale.js"></script>
   <script src="/assets/oclazyload/dist/ocLazyLoad.js"></script>
   <script src="/assets/angular-loading-bar/build/loading-bar.js"></script>
   <script src="/assets/lodash/lodash.js"></script>
   <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/restangular/1.3.1/restangular.min.js"></script>
   <!-- endbuild-->
   <!-- build:js app/js/application.js-->
   <script src="/assets/app.module.js"></script>
   <script src="/assets/modules.js"></script>
  
   <script src="/assets/templates.js"></script>
   
</body>