    <!-- Add your site or application content here -->
    <div>
      <div class="header">
        <div class="navbar navbar-default" role="navigation">
          <div class="container">
            <div class="navbar-header">
  
              <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#js-navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
              </button>
  
              <a class="navbar-brand" href="#/">geckoCli</a>
            </div>
  
            <div class="collapse navbar-collapse" id="js-navbar-collapse" auto-active>
  
              <ul class="nav navbar-nav">
                <li><a ng-href="#/">Home</a></li>
                <li><a href="#/about">About</a></li>
                <li has-permission='!read'><a href="#/user">Usu√°rio</a></li>
                <li has-permission='!read'><a href="#/group">Grupo</a></li>
              </ul>
              
              <div class="oauth" ng-controller="AuthCtrl">
                <a class="oauth__link oauth__link--login" ng-hide="loggedIn" ng-href="{{ loginUrl }}">Login</a><button class="oauth__link oauth__link--logout" ng-click="logout()" ng-show="loggedIn" type="button">Logout</button>
              </div>
            </div>
          </div>
        </div>
      </div>
  
      <div class="container-fluid relative">
      <div ui-view></div>
      </div>
  
      <div class="footer">
        <div class="container">
          <p><span class="glyphicon glyphicon-heart"></span> from the Yeoman team</p>
        </div>
      </div>
    </div>
    <toaster-container></toaster-container>
    <script type="text/javascript" src="/assets/please-wait/build/please-wait.min.js"></script>
    <script type="text/javascript">
      window.loading_screen = window.pleaseWait({
        backgroundColor: '#f46d3b',
        loadingHtml: "<div class='sk-folding-cube'><div class='sk-cube1 sk-cube'></div><div class='sk-cube2 sk-cube'></div><div class='sk-cube4 sk-cube'></div><div class='sk-cube3 sk-cube'></div></div>"
      });
    </script>
   <script src="https://cdnjs.cloudflare.com/ajax/libs/api-check/7.5.5/api-check.min.js"></script>
   <script src="/assets/bower.js"></script>
   <script src="/assets/ngprogress/build/ngprogress.min.js"></script>
   <script src="https://cdnjs.cloudflare.com/ajax/libs/angularjs-toaster/0.4.16/toaster.min.js"></script>
   <script src="/assets/ng-table/dist/ng-table.min.js"></script>
   <script src="/assets/ng-table-to-csv/dist/ng-table-to-csv.js"></script>
   <script src="/assets/angular-formly/dist/formly.js"></script>
   <script src="/assets/angular-formly-templates-bootstrap/dist/angular-formly-templates-bootstrap.js"></script>
   <script src="/assets/sweetalert/dist/sweetalert.min.js"></script>
   <script src="/assets/ngSweetAlert/SweetAlert.min.js"></script>
   <script src="/assets/kendoui/js/kendo.all.min.js"></script>
   <script src="/assets/app.js"></script>
   <%= js_env %>
   <script src="/assets/routes.js"></script>
   <script src="/assets/services.js"></script>
   <script src="/assets/directive.js"></script>
   <script src="/assets/controllers.js"></script>
   <script src="/assets/<%= @theme %>/application.js"></script>
   
   <script type="text/ng-template" id="custom-messages.html">
    <div class="my-messages" ng-messages="options.formControl.$error" ng-if="options.validation.errorExistsAndShouldBeVisible">
        <div class="error-message" ng-message="{{::name}}" ng-repeat="(name, message) in ::options.validation.messages">
            {{message(options.formControl.$viewValue, options.formControl.$modelValue, this)}}
        </div>
    </div>
   </script>
</body>